{
  "name": "ess-webapp-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "VITE_TITLE='Event Stream Service Manager' vite build",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "clean": "rimraf coverage dist",
    "debug": "vite --mode debug",
    "format": "prettier ./src --write",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --no-fix --ignore-path .gitignore",
    "preview": "vite preview",
    "prebuild": "npm run lint",
    "prelint": "npm run typecheck",
    "pretest": "npm run lint",
    "posttest": "node ./lcov-fix.mjs",
    "purge": "rimraf node_modules",
    "rebuild": "npm run clean && npm run build",
    "reinstall": "npm run purge && npm install",
    "serve": "vite",
    "test": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "dev": "vite --port 5173 --host"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^2.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "axios": "^1.8.2",
    "date-fns": "^2.30.0",
    "filesize": "^10.1.0",
    "oidc-client-ts": "^2.3.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.40.1",
    "qrcode.vue": "^3.4.1",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-istanbul": "^2.1.9",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "eslint-plugin-vue": "^9.13.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.4",
    "happy-dom": "^17.4.4",
    "prettier": "3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^6.2.0",
    "vitest": "^2.1.9",
    "volar-service-eslint": "^0.0.15",
    "vue-tsc": "^2.2.8"
  }
}
